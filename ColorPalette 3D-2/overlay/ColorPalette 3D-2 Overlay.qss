QSint--ActionGroup QFrame[class="content"] QPushButton:disabled {
  background-color: rgba(0,0,0,0.2);  
  border: 1px solid rgba(0,0,0,0.2);
}

QSint--ActionGroup QFrame[class="content"] QPushButton:hover {
  background-color: @ThemeAccentColor1;
  background-color: @ColorPaletteHover;
  border: 1px solid @ThemeAccentColor1;
  color: #ffffff;
}

QSint--ActionGroup QFrame[class="content"] QPushButton:pressed {
  background-color: @ThemeAccentColor1;
  border: 1px solid @ThemeAccentColor1;
  color: #ffffff;
}

QSint--ActionGroup QFrame[class="content"] QPushButton:focus {
  border: 1px solid  @ThemeAccentColor1;
}

Gui--OverlayToolButton {
  background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop: 0 rgba(250,250,250,0.15), stop: 0.7 rgba(250,250,250,0.15), stop: 1 rgba(0,0,0,0.1));
  background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop: 0 @ColorPaletteOverlayContent, stop: 0.7 @ColorPaletteOverlayContent, stop: 1 @ColorPaletteOverlayHeader1);
  min-height: 11px;
  min-width: 11px;
  padding: 1px 6px 3px 6px;
  margin: 0px 0px 1px 0px;
  image: url(qss:images_classic/transparent.svg);
  border-width: 1px 1px 2px 1px;
  border-style: solid;
  border-top-color: rgba(250,250,250,0.3);
  border-right-color: rgba(0,0,0,0.2);
  border-bottom-color: rgba(0,0,0,0.6);
  border-left-color: rgba(0,0,0,0.2);
  border-radius: 4px;
}

Gui--OverlayToolButton:hover {
  image: url(qss:images_classic/close-white.svg);
  background-color: @ThemeAccentColor1;
  background-color: @ColorPaletteHover;
}

Gui--OverlayToolButton {
  image: url(qss:images_classic/transparent.svg);
}

Gui--OverlayToolButton:hover {
  image: url(qss:images_classic/transparent.svg);
}

Gui--OverlayToolButton[objectName="OBTN Transparent"] {
  image: url(qss:images_classic/transparent.svg);
}

Gui--OverlayToolButton[objectName="OBTN Transparent"]:hover {
  image: url(qss:images_classic/transparent-white.svg);
}

Gui--OverlayToolButton[objectName="OBTN Overlay"] {
  image: url(qss:images_classic/Transparent.svg);
}

Gui--OverlayToolButton[objectName="OBTN Overlay"]:hover {
  image: url(qss:images_classic/overlay-white.svg);
}

Gui--OverlayToolButton[objectName="OBTN AutoMode"] {
  image: url(qss:images_classic/Transparent.svg);
}

Gui--OverlayToolButton[objectName="OBTN AutoMode"]:hover {
  image: url(qss:images_classic/mode-white.svg);
}
