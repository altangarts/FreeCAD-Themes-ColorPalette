Gui--TaskView--TaskPanel { 
  background-color: transparent;
  border: 1px solid transparent;
  margin: 0px 0px 0px 0px;
  padding: 0px 0px 0px 0px;
  border-radius: 10px;
}

QSint--ActionGroup QFrame[class="content"] {
background-color: rgba(250,250,250,0.07);
background-color: @ColorPaletteOverlayContent;
border: 1px solid rgba(0,0,0,0.15);
border: 1px solid @ColorPaletteOverlayBorder;
border-radius: 6px;
margin: 0px 0px 0px 0px;
padding: -5px;
}

QSint--ActionGroup QFrame[class="header"] {
background-color: rgba(0,0,0,0.2);
background-color: @ColorPaletteOverlayHeader;
border-radius: 6px;
border-width: 1px 1px 2px 1px;
border-style: solid;
border-top-color: rgba(250,250,250,0.3);
border-top-color: @ColorPaletteOverlay3D2Border;
border-right-color: rgba(0,0,0,0.3);
border-right-color: @ColorPaletteOverlay3D2Border1;
border-bottom-color: rgba(0,0,0,0.6);
border-bottom-color: @ColorPaletteOverlay3D2Border2;
border-left-color: rgba(0,0,0,0.3);
border-right-color: @ColorPaletteOverlay3D2Border1;
}

QSint--ActionGroup QFrame[class="header"]:hover {
  background-color: @ThemeAccentColor1;
  color: #ffffff;
}

QSint--ActionGroup QToolButton[class="header"] {
color: #ffaa7f; /* Task Panel Header text color */
color: @ColorPaletteTextHeader;
text-align: left;
font-weight: bold;
border: none;
margin: 0px;
padding: 0px;
}

QSint--ActionGroup QFrame[class="header"] QLabel {
  background-color: transparent;
  border: 0px solid transparent;
  width: 10px;
  height: 10px;
}

QSint--ActionGroup QFrame[class="content"] QListView,
QSint--ActionGroup QFrame[class="content"] QTableView {
color: #ffff69;
color: @ColorPaletteTextList;
margin: 0px 2px 2px 2px;
padding: 5px 0px 5px 5px;
border: 1px solid rgba(0,0,0,0.15);
border-radius: 8px;
background-color: rgba(250,250,250,0.05);
}

QSint--ActionGroup QFrame[class="content"] QTreeView {
color: #ffff69;
color: @ColorPaletteTextList;
margin: 0px 2px 0px 2px;
padding: 5px;
border: 1px solid rgba(0,0,0,0.15);
border-radius: 8px;
background-color: rgba(250,250,250,0.05);
}

QSint--ActionGroup QFrame[class="header"] QLabel[fold="false"] {
  padding: 2px;
  image: url(qss:images_classic/radio-mark-white1.svg);
}

QSint--ActionGroup QFrame[class="header"] QLabel:hover[fold="false"] {
  padding: 2px;
  image: url(qss:images_classic/Arrow-down-white.svg);
}

QSint--ActionGroup QFrame[class="header"] QLabel[fold="true"] {
  padding: 2px;
  image: url(qss:images_classic/Arrow-up-lightgray.svg);
}

QSint--ActionGroup QFrame[class="header"] QLabel[fold="true"]:hover {
  padding: 2px;
  image: url(qss:images_classic/Arrow-up-white.svg);
}

