QMenu::right-arrow {
  margin: 1px;
  padding-left: 12px;
  image:url(qss:images_classic/arrow-right-black1.svg);
  height: 10px;
  width: 10px;
  background-color: transparent;
}

QMenu::right-arrow:selected {
  image:url(qss:images_classic/arrow-right-white1.svg);
}

/* QAbstractItemView ------------------------------------------------------

--------------------------------------------------------------------------- */
QDialog QAbstractItemView {
  alternate-background-color: rgba(0,0,0,0.1);
}

QAbstractItemView QLineEdit {
  background-color: rgba(0,0,0,0.1);
  padding: 2px 4px 2px 4px;
  border: 1px solid rgba(0,0,0,0.15);
  border-radius: 4px;
  color: #eeeeee;
}

/* QAbstractScrollArea ----------------------------------------------------

--------------------------------------------------------------------------- */
QAbstractScrollArea {
  background-color: transparent;
  border: 1px solid transparent;
  padding: 0px;  
  margin: 0px;
}

QAbstractScrollArea:disabled {
  background-color: rgba(0,0,0,0.15);
}

QAbstractScrollArea::corner {
  background-color: transparent;
  border: none;  
}

/* QScrollArea ------------------------------------------------------------

---------------------------------------------------------------------------
QScrollArea QWidget:disabled {
  color: #bbbbbb;
  color: @ColorPaletteTextDisabled;
}

/* QScrollBar -------------------------------------------------------------

--------------------------------------------------------------------------- */
QScrollBar:horizontal {
  background-color: transparent;
  height: 14px;
  margin: 0px;
  padding: 0px 0px -2px 0px;
}

QScrollBar::handle:horizontal {
  background-color: rgba(0,0,0,0.2);
  min-width: 10px;
  border-radius: 2px;
  margin: 8px 14px 3px 14px;
}

QScrollBar::handle:horizontal:hover {
  background-color: @ThemeAccentColor1;
}

QScrollBar::sub-line:horizontal {
  margin: 4px 3px -3px 3px; 
  border-image: url(qss:images_classic/arrow-left-black1.svg);
  width: 9px;
  height: 9px;
  subcontrol-position: left;
  subcontrol-origin: margin;
}

QScrollBar::add-line:horizontal {
  margin: 4px 3px -3px 3px; 
  border-image: url(qss:images_classic/arrow-right-black1.svg);
  width: 9px;
  height: 9px;
  subcontrol-position: right;
  subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal:hover,
QScrollBar::sub-line:horizontal:on {
  border-image: url(qss:images_classic/arrow-left-white1.svg);
}

QScrollBar::add-line:horizontal:hover,
QScrollBar::add-line:horizontal:on {
  border-image: url(qss:images_classic/arrow-right-white1.svg);
}

QScrollBar::up-arrow:horizontal,
QScrollBar::down-arrow:horizontal {
  background-color: none;
}

QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal {
  background-color: transparent;
}

QScrollBar:vertical {
  background-color: transparent;
  width: 15px;
  margin: 0px;
  padding: 0px -1px 0px 0px;
