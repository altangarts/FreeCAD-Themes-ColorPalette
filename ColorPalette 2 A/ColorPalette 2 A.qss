/*==================================================================================================
Settings menu #groupsTreeView:
==================================================================================================*/
QTreeView#groupsTreeView {
  margin: 0px 0px 0px 0px;
  padding: 0px 2px 0px 2px;
  border: none;
  border-radius: 0px;
  selection-background-color: transparent;
  selection-color: #ffffff;
}

QTreeView::item#groupsTreeView {
  background-color: rgba(250,250,250,0.03);
  color: #ffff69;
  color: @ColorPaletteTextTreeEnd;
  border-radius: 0px;
  margin: 1px 0px 1px 0px;
}

QTreeView::item#groupsTreeView:hover,
QTreeView::branch#groupsTreeView:hover {
  background-color: @ThemeAccentColor1;
  background-color: @ColorPaletteHover;
  color: #eeeeee;
}

QTreeView::item#groupsTreeView:selected,
QTreeView::branch#groupsTreeView:selected {
  background-color: @ThemeAccentColor1;
  color: #eeeeee;
}

QTreeView::item#groupsTreeView:selected:hover,
QTreeView::branch#groupsTreeView:selected:hover {
  background-color: @ThemeAccentColor1;
  background-color: @ColorPaletteHover;
  color: #eeeeee;
}

QTreeView::branch#groupsTreeView {
  background-color: rgba(250,250,250,0.03);
  margin: 1px 0px 1px 0px;
}

QTreeView::branch#groupsTreeView:closed:has-children:has-siblings,
QTreeView::item#groupsTreeView:closed:has-children:has-siblings,
