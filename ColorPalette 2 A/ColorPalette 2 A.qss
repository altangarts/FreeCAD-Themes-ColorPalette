QSplitter::handle:horizontal {
  image: url(qss:images_classic/splitter_vertical-black.svg);
}

QSplitter::handle:vertical:hover {
  image: url(qss:images_classic/splitter_horizontal-white.svg);
}

QSplitter::handle:horizontal:hover {
  image: url(qss:images_classic/splitter_vertical-white.svg);
}

/* QDateEdit, QDateTimeEdit -----------------------------------------------

--------------------------------------------------------------------------- */
QDateEdit,
QDateTimeEdit {
  background-color: rgba(0,0,0,0.05);
  selection-background-color: #101010;
  color: #beffff;
  color: @ColorPaletteTextCombobox;
  selection-color: #ffffff;
  border: 1px solid rgba(0,0,0,0.1);
  border-radius: 6px;
  padding: 2px 4px 2px 4px;
  min-width: 50px;
}

QDateEdit:on,
QDateTimeEdit:on {
  selection-background-color: #101010;
  selection-color: #ffffff;
}

QDateEdit::drop-down,
QDateTimeEdit::drop-down {
  subcontrol-origin: padding;
  subcontrol-position: top right;
  background-color: rgba(250,250,250,0.1);
  border-left: 1px solid rgba(0,0,0,0.25);
  width: 18px;
  border-top-right-radius: 6px;
  border-bottom-right-radius: 6px;
  margin: 0px;
}

QDateEdit::down-arrow,
QDateTimeEdit::down-arrow {
  image: url(qss:images_classic/arrow-down-black1.svg);
  width: 10px;
}

QDateEdit::down-arrow:on,
QDateEdit::down-arrow:hover,
QDateEdit::down-arrow:focus,
QDateTimeEdit::down-arrow:on,
QDateTimeEdit::down-arrow:hover {
  image: url(qss:images_classic/arrow-down-white1.svg);
}

QDateEdit QAbstractItemView,
QDateTimeEdit QAbstractItemView {
  background-color: @ThemeAccentColor2;
  border: 1px solid rgba(0,0,0,0.7);
  color: #eeeeee;
  color: @ColorPaletteTextGeneralTop;
  border-radius: 0px;
  selection-background-color: @ThemeAccentColor1;
  selection-color: #ffffff;
}

/* QAbstractView ----------------------------------------------------------

--------------------------------------------------------------------------- */
QAbstractView:hover {
  border: 1px solid @ThemeAccentColor1;
  background-color: @ThemeAccentColor1;
  background-color: @ColorPaletteHover;
  color: #ffffff;
}

QAbstractView:selected {
  border: 1px solid @ThemeAccentColor1;
  background-color: @ThemeAccentColor1;
  color: #ffffff;
}

QAbstractView:focus {
  border: 1px solid @ThemeAccentColor1;
  color: #ffffff;
}

/* PlotWidget -------------------------------------------------------------

--------------------------------------------------------------------------- */
PlotWidget {
  padding: 0px;
  color: rgba(2, 2, 2, 0.614);
}

/*==================================================================================================
SKETCHER
==================================================================================================*/

Gui--StatefulLabel[state="empty_sketch"] {
  color : #ffffff; /* 50% opacity white */
}
Gui--StatefulLabel[state="under_constrained"] {
  color : #ffffff;
}
Gui--StatefulLabel[state="conflicting_constraints"] {
  color : #fc6c6c; /* Red */
  font-weight: bold;
}
Gui--StatefulLabel[state="malformed_constraints"] {
  color : #fc6c6c; /* Red */
  font-weight: bold;
}
Gui--StatefulLabel[state="redundant_constraints"] {
  color : #ffa256; /* Orange red */
}
Gui--StatefulLabel[state="partially_redundant_constraints"] {
  color : #77cbff; /* Royal blue */
}
Gui--StatefulLabel[state="solver_failed"] {
  color : #ff9090; /* Red */
  font-weight: bold;
}
Gui--StatefulLabel[state="fully_constrained"] {
  color : #8cff5b; /* Green */
  font-weight: bold;
}
Gui--UrlLabel {
  color : #aaff00; /* Deep sky blue */
  text-decoration : underline;
}

