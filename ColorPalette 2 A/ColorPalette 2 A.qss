}

QTreeView::branch#groupsTreeView:closed:has-children:has-siblings:hover,
QTreeView::item#groupsTreeView:closed:has-children:has-siblings:hover,
QTreeView::branch#groupsTreeView:closed:has-children:!has-siblings:hover,
QTreeView::item#groupsTreeView:closed:has-children:!has-siblings:hover,
QTreeView::branch#groupsTreeView:open:has-children:has-siblings:hover,
QTreeView::item#groupsTreeView:open:has-children:has-siblings:hover {
