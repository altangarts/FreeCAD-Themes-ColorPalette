  color: #eeeeee;
  color: @ColorPaletteTextGeneralTop;
}

QTreeView::branch#groupsTreeView:open:has-children:has-siblings,
QTreeView::item#groupsTreeView:open:has-children:has-siblings,
QTreeView::branch#groupsTreeView:open:has-children:!has-siblings,
QTreeView::item#groupsTreeView:open:has-children:!has-siblings {
