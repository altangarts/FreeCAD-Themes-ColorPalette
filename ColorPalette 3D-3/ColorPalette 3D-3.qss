QDialog QTableView {
  background-color: rgba(0,0,0,0.05); 
  color: #dddddd;
  margin: 0px 0px 0px 0px;
  border: 1px solid rgba(0,0,0,0.15);
  padding: 6px 6px 6px 6px;
  selection-background-color: @ThemeAccentColor1;
  selection-color: #ffffff;
  border-radius: 6px;
}

.QFrame QTreeView {
  padding: 10px 2px 2px 2px;
  margin: 0px;
  border: none;
}

QDialog QTreeView {
  background-color: rgba(250,250,250,0.05);
  color: #dddddd;
  margin: 0px 8px 0px 8px;
  border: 1px solid rgba(0,0,0,0.15);
  padding: 5px 2px 2px 2px;
  selection-background-color: @ThemeAccentColor1;
  selection-color: #ffffff; 
  border-radius: 6px;  
}

QDialog QTreeView:item {
  margin: 1px 0px 1px 0px;
}

QDialog QTreeView:item:hover {
  background-color: @ThemeAccentColor1;
  border-radius: 0px;
  border: 1px solid @ThemeAccentColor1;
}

QDialog QTreeView:item:selected {
  background-color: @ThemeAccentColor1;
  border-radius: 0px;
  border: 1px solid @ThemeAccentColor1;
  margin: 0px;
}

/* QHeaderView ------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qheaderview

--------------------------------------------------------------------------- */
QHeaderView {
  background-color: transparent;
}

QHeaderView:disabled {
  background-color: #393937;
  color: #aaaaaa;
}

QHeaderView::section {
  background-color: rgba(0,0,0,0.2);
  color: #eeeeee;
  border: 1px solid rgba(0,0,0,0.2);
  border-radius: 4px;
}

QHeaderView::section:hover {
  background-color: @ThemeAccentColor1;
  color: #ffffff;
}

QTableCornerButton::section {
  background-color: @ThemeAccentColor1;
  color: #eeeeee;
  border: 1px solid rgba(0,0,0,0.2);
  border-radius: 4px;
}

QHeaderView::section::horizontal {
  padding: 3px 3px 3px 10px;
  margin: 0px 1px 0px 1px;
}

QHeaderView::section::horizontal::first, QHeaderView::section::horizontal::only-one {
  /* border-left: 0px solid #020202; */
}

QHeaderView::section::horizontal:disabled {
  color: #aaaaaa;
}

QHeaderView::section::vertical {
  padding: 0px 0px 0px 0px;
  margin: 1px 0px 1px 0px;
}

QHeaderView::section::vertical::first, QHeaderView::section::vertical::only-one {
  /* border-top: 2px solid #020202; */
}

QHeaderView::section::vertical:disabled {
  color: #aaaaaa;
}

QHeaderView::down-arrow {
  background-color: transparent;
  border: none;
  height: 12px;
  width: 12px;
  padding-left: 2px;
  padding-right: 2px;
  image: url(qss:images_classic/Arrow-down-white.png);
}

QHeaderView::up-arrow {
  background-color: transparent;
  border: none;
  height: 12px;
  width: 12px;
  padding-left: 2px;
  padding-right: 2px;
  image: url(qss:images_classic/Arrow-up-white.png);
}

/* QToolBox --------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qtoolbox
used in PATH

--------------------------------------------------------------------------- */
QToolBox {
  padding: 0px;
  border: 0px solid transparent;
  border-radius: 4px;
  background-color: @ThemeAccentColor3;
  color: #dddddd;
}

QToolBox:selected {
  padding: 0px;
  border: 1px solid @ThemeAccentColor1;
  color: #ffffff;
}

QToolBox::tab {
  background-color: @ThemeAccentColor2;
  color: #eeeeee;
  border-radius: 4px;
  border: 1px solid #505050;
  image: none;
  background-image: none;
  padding-left: 0px;
}

QToolBox::tab:disabled {
  color: #aaaaaa;
}

QToolBox::tab:selected {
  background-color: @ThemeAccentColor1;
  color: #ffffff;
}

QToolBox::tab:selected:disabled {
  background-color: #393937;
  border-bottom: 0px solid #020202;
  color: #aaaaaa;
}

QToolBox::tab:!selected {
  background-color: @ThemeAccentColor1;
  color: #ffffff;
}

QToolBox::tab:!selected:disabled {
  background-color: #393937;
}

QToolBox::tab:hover {
  background-color: @ThemeAccentColor1;
  color: #ffffff;
  }

QToolBox > QScrollArea > QWidget > QWidget {
  padding: 0px;
  border: 0px;
  background-color: @ThemeAccentColor2;
  border-radius: 4px;
}

/* QFrame -----------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qframe
https://doc.qt.io/qt-5/qframe.html#-prop
https://doc.qt.io/qt-5/qframe.html#details
https://stackoverflow.com/questions/14581498/qt-stylesheet-for-hline-vline-color

--------------------------------------------------------------------------- */
/* (dot) .QFrame  fix #141, #126, #123 */
.QFrame {
}

.QFrame[frameShape="0"] {
  background-color: rgba(250,250,250,0.05);
  border-left: 1px solid rgba(0,0,0,0.25);
  border-radius: 0px;
  margin: 0px 0px 0px 0px;
}

.QFrame[frameShape="4"] {
  max-height: 1px;
  border: none;
  background-color: rgba(0,0,0,0.0);
}

.QFrame[frameShape="5"] {
  max-width: 1px;
  border: none;
  background-color: rgba(0,0,0,0.0);
}

/* QSplitter --------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qsplitter

---------------------------------------------------------------------------*/
QSplitterHandle:hover { 
}

QSplitter::handle {
}

QSplitter::handle:horizontal {
  background-position: center center;
  background-repeat: none;  
  width: 1px;
  padding: 3px;
}

QSplitter::handle:vertical {
  background-color: transparent;
  border-top: 0px solid transparent;
  border-bottom: 0px solid transparent;
  background-position: center center;
  background-repeat: none;
  height: 4px;
  padding: 1px 0px 0px 0px;
}

QSplitter::handle:vertical {
  image: url(qss:images_classic/splitter_horizontal-black.png);
}

QSplitter::handle:horizontal {
  image: url(qss:images_classic/splitter_vertical-black.png);
}

QSplitter::handle:vertical:hover {
  image: url(qss:images_classic/splitter_horizontal-white.png);
}

QSplitter::handle:horizontal:hover {
  image: url(qss:images_classic/splitter_vertical-white.png);
}

/* QDateEdit, QDateTimeEdit -----------------------------------------------

--------------------------------------------------------------------------- */
QDateEdit, QDateTimeEdit {
  selection-background-color: @ThemeAccentColor1;
  selection-color: #ffffff;
  border-style: solid;
  border: 1px solid #505050;
  border-radius: 4px;
  padding: 2px 4px 2px 4px;
  min-width: 10px;
}

QDateEdit:on, QDateTimeEdit:on {
  selection-background-color: @ThemeAccentColor1;
  selection-color: #ffffff;
}

QDateEdit::drop-down, QDateTimeEdit::drop-down {
  subcontrol-origin: padding;
  subcontrol-position: top right;
  width: 12px;
  border-left: 1px solid #505050;
}

QDateEdit::down-arrow, QDateTimeEdit::down-arrow {
  image: url(qss:images_classic/Arrow-down-black.png);
  height: 8px;
  width: 8px;
}

QDateEdit::down-arrow:on, QDateEdit::down-arrow:hover, QDateEdit::down-arrow:focus, QDateTimeEdit::down-arrow:on, QDateTimeEdit::down-arrow:hover {
  image: url(qss:images_classic/Arrow-down-white.png);
}

QDateEdit QAbstractItemView, QDateTimeEdit QAbstractItemView {
  background-color: @ThemeAccentColor2;
  border-radius: 2px;
  border: 1px solid #505050;
  selection-background-color: @ThemeAccentColor1;
  selection-color: #ffffff;
}

/* QAbstractView ----------------------------------------------------------

--------------------------------------------------------------------------- */
QAbstractView:hover {
  border: 1px solid @ThemeAccentColor1;
  color: #ffffff;
}

QAbstractView:selected {
  background-color: @ThemeAccentColor1;
  color: #ffffff;
}

/* PlotWidget -------------------------------------------------------------

--------------------------------------------------------------------------- */
PlotWidget {
  padding: 0px;
  color: rgba(2, 2, 2, 0.614);
}

