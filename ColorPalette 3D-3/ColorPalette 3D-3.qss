/*
ABOUT
============================================================================================================
Revised stylesheet based on the original from Pablo Gil FernÃ¡ndez made by MisterMaker

KNOWN BUGS and TO DO
============================================================================================================
Please, follow the link to get updated information:
https://forum.freecad.org/viewtopic.php?t=78431

INSTALLATION WHEN NECESSARY
============================================================================================================
1) Place the .qss files and /images/ folder in the path that fits your OS:
    OSX = /Users/[YOUR_USER_NAME]/Library/Preferences/FreeCAD/Gui/Stylesheets/
    WINDOWS = C:/[INSTALLATION_PATH]/FreeCAD/data/Gui/Stylesheets/
    LINUX = /home/[YOUR_USER_NAME]/.FreeCAD/Gui/Stylesheets/

============================================================================================================
FOLLOWING CODES ARE CHANGED IN THE SETTINGS:
  @ThemeAccentColor1
  @ThemeAccentColor2
  @ThemeAccentColor3
See Qt documentation:

  - https://doc.qt.io/qt-5/stylesheet.html
  - https://doc.qt.io/qt-5/stylesheet-reference.html
  - https://doc.qt.io/qt-5/stylesheet-examples.html

--------------------------------------------------------------------------- */


/* Reset elements ------------------------------------------------------------

Resetting everything helps to unify styles across different operating systems

--------------------------------------------------------------------------- */
* {
  padding: 0px;
  margin: 0px;
  border: 0px;
  border-style: none;
  border-image: none;
  outline: 0;
  selection-background-color: rgba(0,0,0,0.7);
  selection-color: #ffffff;
}

/* specific reset for elements inside QToolBar */
QToolBar * {
  margin: 0px;
  padding: 0px;
}

Gui--PropertyEditor--PropertyEditor QLineEdit:read-only,
Gui--PropertyEditor--PropertyEditor QLineEdit:disabled,
Gui--PropertyEditor--PropertyEditor QAbstractSpinBox:read-only,
Gui--PropertyEditor--PropertyEditor QAbstractSpinBox:disabled {
    color: #aaaaaa;
    border: 1px solid @ThemeAccentColor1;
    background-color: @ThemeAccentColor1;
    selection-background-color: rgba(0,0,0,0.7);
    selection-color: #ffffff;
    border-radius: 0px;
}

Gui--PropertyEditor--PropertyEditor > QWidget > QLabel,
Gui--PropertyEditor--PropertyEditor > QWidget > QLabel:disabled,
Gui--PropertyEditor--PropertyEditor > QWidget > QLineEdit {
    background-color: @ThemeAccentColor1;
    border: 1px solid @ThemeAccentColor1;
    border-radius: 0px;
    margin: 0px;
    padding: 0px;
}

Gui--PropertyEditor--PropertyEditor QSpinBox,
Gui--PropertyEditor--PropertyEditor QDoubleSpinBox,
Gui--PropertyEditor--PropertyEditor QAbstractSpinBox,
Gui--PropertyEditor--PropertyEditor QLineEdit,
Gui--PropertyEditor--PropertyEditor QComboBox,
Gui--PropertyEditor--PropertyEditor QPushButton {
    background-color: @ThemeAccentColor1;
    border: 0px solid rgba(0,0,0,0.2);
    margin: 0px;
    padding: 0px 0px 2px 6px;
    border-radius: 0px;
}

Gui--PropertyEditor--PropertyEditor QSpinBox:selected,
Gui--PropertyEditor--PropertyEditor QDoubleSpinBox:selected,
Gui--PropertyEditor--PropertyEditor QAbstractSpinBox:selected,
Gui--PropertyEditor--PropertyEditor QLineEdit:selected,
Gui--PropertyEditor--PropertyEditor QComboBox:selected,
Gui--PropertyEditor--PropertyEditor QPushButton:selected {
    background-color: @ThemeAccentColor1;
    border: 0px solid @ThemeAccentColor1;
    margin: 0px;
    border-radius: 0px;
}

Gui--PropertyEditor--PropertyEditor QComboBox::drop-down {
  border: 1px solid rgba(0,0,0,0.2);
}

Gui--PropertyEditor--PropertyEditor QComboBox::down-arrow {
  image: url(qss:images_classic/Arrow-down-white.png);
}

Gui--PropertyEditor--PropertyEditor QAbstractSpinBox::up-button {
  border: 1px solid rgba(0,0,0,0.2);
}

Gui--PropertyEditor--PropertyEditor QAbstractSpinBox::up-arrow {
  image: url(qss:images_classic/Arrow-up-white.png);
}

Gui--PropertyEditor--PropertyEditor QAbstractSpinBox::down-button {
  border: 1px solid rgba(0,0,0,0.2);
}

Gui--PropertyEditor--PropertyEditor QAbstractSpinBox::down-arrow {
  image: url(qss:images_classic/Arrow-down-white.png);
}

Gui--PropertyEditor--PropertyEditor > QWidget > QFrame:focus {
    background-color: @ThemeAccentColor1; 
    color: #ffffff;
    border-radius: 0px;
}

Gui--PropertyEditor--PropertyEditor > QWidget > QWidget > QLabel {
    background-color: #ab4700;
    color: #ffffff;
    border-radius: 0px;
}

/*==================================================================================================
Main window
==================================================================================================*/
QMainWindow,
QDialog,
QDockWidget {
  background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop: 0 @ThemeAccentColor2, stop: 0.06 @ThemeAccentColor2, stop: 0.5 @ThemeAccentColor3, stop: 1 @ThemeAccentColor3); /* main background color */
}

QMdiArea {
  background-image: url(qss:images_dark-light/background_freecad.svg);
   background-position: center center;
   background-repeat: no-repeat;
}

Gui--NavigationIndicatorGui--BlenderNavigationStyle {
 qproperty-icon: url(:/icons/icons/NavigationBlender_light.svg);
}

/* QDockWiget -------------------------------------------------------------

--------------------------------------------------------------------------- */
QDockWidget {
  background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop: 0 @ThemeAccentColor2, stop: 0.5 @ThemeAccentColor3, stop: 1 @ThemeAccentColor3);
  titlebar-close-icon: url(qss:images_classic/transparent.png);
  titlebar-normal-icon: url(qss:images_classic/transparent.png);
}

QDockWidget QFrame {
  background-color: rgba(250,250,250,0.015);
  border: 1px solid rgba(0,0,0,0.1);
  margin: 0px 2px 0px 2px;
  padding: 0px -5px 0px 0px;
  border-radius: 10px;
}

QDockWidget QWidget::pane {
  background-color: transparent;
  border: 1px solid transparent;
  margin: 0px;
  padding: 0px;
}

QDockWidget QTabBar {
  background-color: transparent;
  border: 1px solid transparent;
}

QDockWidget::title {
}

QDockWidget::close-button {
  image: url(qss:images_classic/close-white.png);
}

QDockWidget::close-button:hover {
  image: url(qss:images_classic/close-red.png);
}

QDockWidget::close-button:pressed {
  image: url(qss:images_classic/close-white.png);
}

QDockWidget::float-button {
  icon-size: 20px;
  border: none;
  background-color: @ThemeAccentColor2;
  background-image: none;
  margin: 0px;
  padding: 0px;
  image: url(qss:images_classic/undock-white.png);
}

QDockWidget::float-button:hover {
  image: url(qss:images_classic/undock-blue.png);
}

QDockWidget::float-button:pressed {
  image: url(qss:images_classic/undock-white.png);
}

Gui--OverlayTitleBar {
  color: transparent;
}

Gui--OverlayToolButton {
  background-color: rgba(250,250,250,0.15); 
  min-height: 10px;
  min-width: 10px;
  padding: 1px 6px 3px 6px;
  margin: 0px 2px 3px 0px;
  image: url(qss:images_classic/close-black.png);
  border-width: 1px 1px 2px 1px;
  border-style: solid;
  border-top-color: rgba(250,250,250,0.3);
  border-right-color: rgba(0,0,0,0.3);
  border-bottom-color: rgba(0,0,0,0.6);
  border-left-color: rgba(0,0,0,0.3);
  border-radius: 4px;
}

Gui--OverlayToolButton:hover {
  image: url(qss:images_classic/close-white.png);
  background-color: @ThemeAccentColor1;
  border: 1px solid rgba(0,0,0,0.5);
}

Gui--OverlayToolButton[objectName="OBTN Float"] {
  image: url(qss:images_classic/float-black.png);
}

Gui--OverlayToolButton[objectName="OBTN Float"]:hover {
  image: url(qss:images_classic/float-white.png);
}

Gui--OverlayToolButton[objectName="OBTN Overlay"] {
  image: url(qss:images_classic/overlay-black.png);
}

Gui--OverlayToolButton[objectName="OBTN Overlay"]:hover {
  image: url(qss:images_classic/overlay-white.png);
} 

/* QWidget ----------------------------------------------------------------
This causes much load time.
--------------------------------------------------------------------------- */
QColorDialog QWidget {
  background-color: rgba(0,0,0,0.2);
  color: #eeeeee;
  selection-background-color: rgba(0,0,0,0.7);
  selection-color: #ffffff;
}

QColorDialog QMenu {
  margin: 0px;
  padding: 4px 0px 2px 2px;
  border: 1px solid rgba(0,0,0,0.2);
  color: #dddddd;  
  background-color: @ThemeAccentColor2;
}

QColorDialog QLabel {   
  background-color: transparent;
  color: #dddddd; 
  margin-right: 0px;
  padding-right: 0px;
}

/* QMainWindow ------------------------------------------------------------

This adjusts the splitter in the dock widget, not qsplitter
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qmainwindow

--------------------------------------------------------------------------- */
QMainWindow::separator {
  border-right: 0px solid rgba(0,0,0,0.2);
  spacing: 0px;
  padding: 0px;
}

QMainWindow::separator:vertical {
  image: url(qss:images_classic/splitter_vertical-black.png);
}

QMainWindow::separator:horizontal {
  image: url(qss:images_classic/splitter_horizontal-black.png);
}

QMainWindow::separator:vertical:hover {
  image: url(qss:images_classic/splitter_vertical-white.png);
}

QMainWindow::separator:horizontal:hover {
  image: url(qss:images_classic/splitter_horizontal-white.png);
}

QMainWindow::separator:horizontal {
  height: 2px;
  margin-top: 1px;
  margin-bottom: 1px;
}

QMainWindow::separator:vertical {  
  width: 4px;
  margin-left: 1px;
  margin-right: 1px;
}

/* QToolTip ---------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qtooltip

--------------------------------------------------------------------------- */
QToolTip {
  background-color: #aa5500;
  color: #ffffff;
  border-width: 1px;
  border-style: solid;
  border-color: #202020;
  padding: 2px;
}

/* QStatusBar -------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qstatusbar

--------------------------------------------------------------------------- */
QStatusBar {
  border: 0px solid rgba(0,0,0,0.2);
  background-color: @ThemeAccentColor3;
  spacing: 0px;
}

QStatusBar::item {
  border: none;
}

QStatusBar QToolTip {
  background-color: #aa5500;
  border: 1px solid transparent;
  color: #ffffff;
  padding: 0px;
  opacity: 230;
}

QStatusBar QLabel {
  background-color: transparent;
}

/* QCheckBox --------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qcheckbox

--------------------------------------------------------------------------- */
QCheckBox {
  color: #aef088;
  padding: 0px;
  outline: none;
  background-color: transparent;
  border: 1px solid transparent;
  spacing: 8px;
}

QCheckBox:hover {
  border: 1px solid transparent;
  border-radius: 2px;
  color: #ffffff;
}

QCheckBox QWidget:disabled {
  color: #777777;
}

QCheckBox::indicator {
  background-color: rgba(0,0,0,0.6);
  border: 1px solid rgba(0,0,0,0.8);
  border-radius: 2px;
  width: 14px;
  height: 14px;
}

QCheckBox::indicator:unchecked {
  background-color: rgba(0,0,0,0.6);
  border: 1px solid rgba(0,0,0,0.8);
}

QCheckBox::indicator:unchecked:hover,
QCheckBox::indicator:unchecked:pressed {
  background-color: @ThemeAccentColor1;
  border: 1px solid rgba(0,0,0,0.8);
}

QCheckBox::indicator:unchecked:disabled {
  background-color: #393937;
  border: 1px solid #333331;
}

QCheckBox::indicator:checked {
  image:url(qss:images_classic/check-mark-blue.png);
  background-color: rgba(0,0,0,0.6);
  border: 1px solid rgba(0,0,0,0.8);
}

QCheckBox::indicator:checked:hover, 
QCheckBox::indicator:checked:pressed {
  image:url(qss:images_classic/check-mark-white.png);
  background-color: @ThemeAccentColor1;
  border: 1px solid rgba(0,0,0,0.8);
}


QCheckBox::indicator:checked:disabled {
  background-color: #393937;
  border: 1px solid #333331;
  image: url(qss:images_classic/check-mark-lightgray.png);
}

QCheckBox::indicator:indeterminate {
  background-color: rgba(0,0,0,0.6);
  border: 1px solid rgba(0,0,0,0.8);
  image: url(qss:images_classic/transparent.png);
}

QCheckBox::indicator:indeterminate:disabled {
  background-color: #393937;
  border: 1px solid #333331;
  image: url(qss:images_classic/transparent.png);
}

QCheckBox::indicator:indeterminate:hover, 
QCheckBox::indicator:indeterminate:pressed {
  background-color: @ThemeAccentColor1;
  border: 1px solid rgba(0,0,0,0.8);
}

/* QGroupBox --------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qgroupbox

--------------------------------------------------------------------------- */
QGroupBox {
  background-color: rgba(0,0,0,0.07);
  border: 1px solid rgba(0,0,0,0.2);
  border-radius: 6px;
  margin-top: 10px;
  padding-top: 5px;
  font-weight: bold;
}

QGroupBox::title {
  top: -8px;
  left: 15px;
  color: #000000;
}

QGroupBox::indicator {
  background-color: rgba(0,0,0,0.6);
  width: 14px;
  height: 14px;
  border: 1px solid transparent;
  border-radius: 2px;
}

QGroupBox::indicator:unchecked {
  background-color: rgba(0,0,0,0.6);  
  border: 1px solid rgba(0,0,0,0.8);
}

QGroupBox::indicator:unchecked:hover {
  background-color: @ThemeAccentColor1;
  border: 1px solid rgba(0,0,0,0.8);
}

QGroupBox::indicator:unchecked:disabled {
  background-color: #393937;
  border: 1px solid #333331;
}

QGroupBox::indicator:checked {
  background-color: rgba(0,0,0,0.6);
  border: 1px solid rgba(0,0,0,0.8);
  image:url(qss:images_classic/check-mark-blue.png);
}

QGroupBox::indicator:checked:hover {
  background-color: @ThemeAccentColor1;
  border: 1px solid rgba(0,0,0,0.8);
  image:url(qss:images_classic/check-mark-white.png);
}

QGroupBox::indicator:checked:disabled {
  background-color: #393937;
  image:url(qss:images_classic/check-mark-lightgray.png);
  border: 1px solid #333331;
}

/* QRadioButton -----------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qradiobutton

--------------------------------------------------------------------------- */
QRadioButton {
  background-color: transparent;
  color: #ffbf00;
  border: none;
  spacing: 8px;
  outline: none;
}

QRadioButton:hover {
  color: #ffffff;
}

QRadioButton:disabled {
  background-color: transparent;
  color: #aaaaaa;
}

QRadioButton QWidget {
  background-color: transparent;
  spacing: 0px;
  padding: 0px;
  outline: none;
  border: none;
}

QRadioButton::indicator {
  border-image: none;
  width: 14px;
  height: 14px;
  image:url(qss:images_classic/Radio-unchecked-lightgray.png);
}

QRadioButton::indicator:unchecked {
}

QRadioButton::indicator:unchecked:hover {
  image:url(qss:images_classic/Radio-unchecked-black.png);
}

QRadioButton::indicator:unchecked:disabled {
  border-image: none;
  width: 14px;
  height: 14px;
  image:url(qss:images_classic/Radio-unchecked-darkgray.png);
}

QRadioButton::indicator:checked {
  border-image: none;
  width: 14px;
  height: 14px;
  image:url(qss:images_classic/Radio-unchecked-white.png);
}

QRadioButton::indicator:checked:disabled {
  border-image: none;
  width: 14px;
  height: 14px;
  image:url(qss:images_classic/Radio-checked-darkgray.png);
}

/* QMenuBar ---------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qmenubar

--------------------------------------------------------------------------- */
QMenuBar {
  background-color: @ThemeAccentColor2;
  selection-background-color: @ThemeAccentColor1;
  selection-color: #ffffff;
  spacing: 0px;
}

QMenuBar::item {
  background-color: transparent;
  border: 1px solid transparent;
  border-radius: 4px;
  color: #dddddd;
  padding: 2px 6px 4px 6px;
  margin: 2px 0px 2px 0px;
}

QMenuBar::item:hover {
  background-color: @ThemeAccentColor1;
  border: 1px solid @ThemeAccentColor1;
  color: #ffffff;
}

QMenuBar::item:selected {
  background-color: @ThemeAccentColor1;
  border: 1px solid @ThemeAccentColor1;
  color: #ffffff;
}

QMenuBar::item:pressed {
  background-color: @ThemeAccentColor1;
  border: 1px solid @ThemeAccentColor1;
  color: #ffffff;
}

/* QMenu ------------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qmenu

--------------------------------------------------------------------------- */
QMenu {
  margin: 0px;
  padding: 1px 2px 1px 2px;
  border: 1px solid rgba(0,0,0,0.7);
  color: #dddddd;  
  background-color: @ThemeAccentColor2;
}

QMenu::separator {
  height: 1px;
  background-color: rgba(0,0,0,0.3);
  margin-left: 10%;
  margin-right: 10%;
  margin-top: 2px;
  margin-bottom: 2px;
}

QMenu::item {
  background-color: transparent;
  padding: 3px 10px 3px 6px;
  margin: 1px 0px 1px 0px;
  border-radius: 4px;
}

QMenu::item:selected {
  background-color: @ThemeAccentColor1;
  color: #ffffff;
}

QMenu::item:hover {
  background-color: @ThemeAccentColor1;
  color: #ffffff;
}

QMenu::item:pressed {
  background-color: @ThemeAccentColor1;
  color: #ffffff;
}

QMenu::item:disabled {
  background-color: rgba(0,0,0,0.07);
  color: #aaaaaa;
}

QMenu::icon {
  margin-left: 10px;
  width: 14px;
  height: 14px;
}

QMenu::indicator {
  background-color: rgba(0,0,0,0.6);
  border: 1px solid rgba(0,0,0,0.8);
  border-radius: 2px;
  margin-left: 4px;
  margin-right: 4px;
  width: 14px;
  height: 14px;  
}

QMenu::icon:checked {
  background-color: @ThemeAccentColor1;
  border: 2px solid @ThemeAccentColor1;
  position: absolute;
  border-radius: 4px;
  margin-left: 4px;
  margin-right: 4px;
}

QMenu::indicator:non-exclusive:unchecked {
  background-color: rgba(0,0,0,0.6);
}

QMenu::indicator:non-exclusive:unchecked:hover, 
QMenu::indicator:non-exclusive:unchecked:focus, 
QMenu::indicator:non-exclusive:unchecked:pressed {
  background-color: @ThemeAccentColor1;
  border: 1px solid rgba(0,0,0,0.8);
}

QMenu::indicator:non-exclusive:unchecked:disabled {
}

QMenu::indicator:non-exclusive:checked {
  image:url(qss:images_classic/check-mark-blue.png);
}

QMenu::indicator:non-exclusive:checked:hover,
QMenu::indicator:non-exclusive:checked:focus,
QMenu::indicator:non-exclusive:checked:pressed {
  border: 1px solid rgba(0,0,0,0.8);
  background-color: @ThemeAccentColor1;
  image:url(qss:images_classic/check-mark-white.png);
}

QMenu::indicator:non-exclusive:checked:disabled {
image:url(qss:images_classic/check-mark-lightgray.png);
}

QMenu::indicator:exclusive:unchecked {
  image: url(qss:images_classic/transparent.png);
}

QMenu::indicator:exclusive:unchecked:hover,
QMenu::indicator:exclusive:unchecked:focus,
QMenu::indicator:exclusive:unchecked:pressed {
  border: none;
  outline: none;
  background-color: @ThemeAccentColor1;
}

QMenu::indicator:exclusive:unchecked:disabled {
}

QMenu::indicator:exclusive:checked {
  border: 1px solid rgba(0,0,0,0.8);
  outline: none;
  image: url(qss:images_classic/check-mark-blue.png);
}

QMenu::indicator:exclusive:checked:hover,
QMenu::indicator:exclusive:checked:focus,
QMenu::indicator:exclusive:checked:pressed {
  border: none;
  outline: none;
  background-color: @ThemeAccentColor1;
  image: url(qss:images_classic/check-mark-white.png);
}

QMenu::indicator:exclusive:checked:disabled {
  outline: none;
  image: url(qss:images_classic/check-mark-lightgray.png);
}

QMenu::right-arrow {
  margin: 1px;
  padding-left: 12px;
  image:url(qss:images_classic/Arrow-right-black.png);
  height: 10px;
  width: 10px;
  background-color: transparent;
}

QMenu::right-arrow:selected {
  image:url(qss:images_classic/Arrow-right-white.png);
}

/* QAbstractItemView ------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qcombobox

--------------------------------------------------------------------------- */
QAbstractItemView {
  alternate-background-color: rgba(0,0,0,0.15); /* TreeView Row */
}

QAbstractItemView QLineEdit {
  padding: 0px;
}

/* QAbstractScrollArea ----------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qabstractscrollarea

--------------------------------------------------------------------------- */
QAbstractScrollArea {
  background-color: transparent;
  border: 0px solid transparent;
  padding: 0px;  
  margin: 0px;
}

QAbstractScrollArea:disabled {
  background-color: #393937;
  color: #aaaaaa;
}

QAbstractScrollArea::corner {
  border: none;
  background-color: transparent;
}

/* QScrollArea ------------------------------------------------------------

---------------------------------------------------------------------------
text input field disabled!!!!*/
QScrollArea QWidget:disabled {
  /* background-color: #393937; */
    color: #aaaaaa;
}

/* QScrollBar -------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qscrollbar

--------------------------------------------------------------------------- */
QScrollBar:horizontal {
  background-color: transparent;
  height: 14px;
  margin: 0px;
  padding: 0px 0px -2px 0px;
}

QScrollBar::handle:horizontal {
  background-color: rgba(0,0,0,0.2);
  min-width: 10px;
  border-radius: 2px;
  margin: 8px 14px 3px 14px;
}

QScrollBar::handle:horizontal:hover {
  background-color: @ThemeAccentColor1;
}

QScrollBar::sub-line:horizontal {
  margin: 4px 3px -3px 3px; 
  border-image: url(qss:images_classic/Arrow-left-black.png);
  width: 9px;
  height: 9px;
  subcontrol-position: left;
  subcontrol-origin: margin;
}

QScrollBar::add-line:horizontal {
  margin: 4px 3px -3px 3px; 
  border-image: url(qss:images_classic/Arrow-right-black.png);
  width: 9px;
  height: 9px;
  subcontrol-position: right;
  subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal:hover,
QScrollBar::sub-line:horizontal:on {
  border-image: url(qss:images_classic/Arrow-left-white.png);
}

QScrollBar::add-line:horizontal:hover,
QScrollBar::add-line:horizontal:on {
  border-image: url(qss:images_classic/Arrow-right-white.png);
}

QScrollBar::up-arrow:horizontal,
QScrollBar::down-arrow:horizontal {
  background-color: none;
}

QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal {
  background-color: transparent;
}

QScrollBar:vertical {
  background-color: transparent;
  width: 15px;
  margin: 0px;
  padding: 0px -1px 0px 0px;
}

QScrollBar::handle:vertical {
  background-color: rgba(0,0,0,0.2);
  min-height: 10px;
  border-radius: 2px;
  margin: 14px 3px 14px 8px;
}

QScrollBar::handle:vertical:hover {
  background-color: @ThemeAccentColor1;
}

